    "icons": "npm run icons:opt && npm run icons:clean && npm run icons:build"
  },
  "dependencies": {
    "framer-motion": "^12.23.15",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@svgr/cli": "^8.1.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "rimraf": "^6.0.1",
    "svgo": "^4.0.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}