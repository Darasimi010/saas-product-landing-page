{
  "name": "saas-product-landing-page",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "icons:clean": "rimraf src/icons",
    "icons:opt": "svgo -f src/assets/icons",
